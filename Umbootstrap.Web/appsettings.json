{
  "$schema": "./appsettings-schema.json",
  "BlockPreview": {
    "BlockGrid": {
      "Enabled": true,
      "ContentTypes": [
        "featurePageTitleDescription",
        "featureRichTextEditor",
        "featureImage",
        "featureInternalLinksChildren",
        "featureInternalLinks",
        "featureInternalLinksSlideshow",
        "featureInternalLinksPagination",
        "featureNavigationDescendants",
        "featureFaqs",
        "featureTabs",
        "featureHtml",
        "featureCode",
        "featureFormContactUs"
      ],
      "Stylesheets": [ "/css/Index.css" ]
    },
    "BlockList": {
      "Enabled": false
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto"
      },
      "Global": {
        "TimeOut": "01:00:00",
        "Id": "0f2709dd-6052-4c76-b78c-2e2a2da6e8c4",
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      }
    }
  },
  "uSync": {
    "Settings": {
      //"ImportAtStartup": "All"
      "ImportOnFirstBoot": true
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
  }
}